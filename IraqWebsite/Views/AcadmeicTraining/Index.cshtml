@model IraqWebsite.ViewModels.AcadmeicTraining.AcadmeicTrainingDto
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class=" position-relative " style="background: url('/Images/pic.jpg'); height:40vh; background-size: cover;  ">
        <div class="overlay" style="position: absolute; top: 0px; left: 0px; height: 100%; width: 100%; background: rgba(0, 0, 0, .5); "></div>
    <div class="container h-100" style="z-index:4; position:relative">
        <div class="breadcumb-content h-100 d-flex align-items-end pb-5">
            <h1 class="breadcumb-title">@ViewData["AcedemicTraining"]</h1>
                <div class="breadcumb-menu-wrap">
                    
                </div>
            </div>
        </div>
    </div>

<section class=" space-top space-extra-bottom">
<div class="container">
    <div class="row flex-row-reverse">
        <div class="col-lg-8">
            <div class="mb-3 pb-3"><img src="@Model.ImageOne" style="height: 490px; width: 100%; object-fit: cover;" alt="Project image"></div>
            <h2 class="h4">@Model.Title</h2>
            <p>@Model.Description</p>
                 
            <div class="row pt-3 mb-30 pb-10 gy-30   wow fadeInUp" data-wow-delay="0.2s">
                <div class="col-md-6"><img src="@Model.ImageTwo" style="object-fit: cover; height: 350px;" alt="project image"></div>
                <div class="col-md-6"><img src="@Model.ImageThree" style="object-fit: cover; height: 350px;" alt="project image"></div>
            </div>

        </div>
        <div class="col-lg-4 put-rtl">
            <aside class="service-sideba r">
                <div class="quote-box" style="background: url(./assets/img/2.jpg) center; background-size: cover;">
                    <h3 class="quote-box__title">@ViewData["AnyQuestions"]</h3>
                        <a asp-controller="ContactUs" asp-action="Index" class="vs-btn">@ViewData["ContactUs"]<i class="far fa-arrow-right"></i></a>
                </div>
            </aside>
        </div>
    </div>
</div>
</section>